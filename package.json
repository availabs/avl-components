{
  "name": "@availabs/avl-components",
  "version": "1.0.5",
  "description": "Reusable components styled by tailwind css for use in AVAIL projects.",
  "main": "dist/index.js",
  "scripts": {
    "test": "node test.js",
    "build": "babel src -d dist",
    "publish": "npm publish --access public",
    "watch": "nodemon --exec npm run build"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      ".git"
    ],
    "ext": "js, jsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/availabs/avl-components.git"
  },
  "keywords": [
    "components",
    "avail",
    "availabs",
    "avl",
    "AVAIL",
    "AVAILABS"
  ],
  "author": "AVAILABS",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/availabs/avl-components/issues"
  },
  "homepage": "https://github.com/availabs/avl-components#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5"
  },
  "dependencies": {
    "d3-brush": "^1.1.5",
    "d3-selection": "^1.4.1",
    "deep-equal": "^2.0.4",
    "falcor": "^2.2.0",
    "falcor-http-datasource": "^0.1.3",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "match-sorter": "^5.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-dom": "^16.13.1",
    "react-markdown-renderer": "^1.4.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-table": "^7.6.2",
    "styled-components": "^5.2.1"
  }
}
